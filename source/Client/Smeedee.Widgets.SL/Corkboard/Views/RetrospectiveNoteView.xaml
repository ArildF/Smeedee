                        x:Name="UpArrow" 
                        Margin="0" 
                        Template="{StaticResource Uparrow2}" 
                        MaxHeight="45" 
                        Command="{Binding MoveUp}"/>
                        
                	<Button 
                        x:Name="DownArrow" 
                        Margin="0" 
                        Template="{StaticResource Uparrow2}" 
                        RenderTransformOrigin="0.5,0.5" 
                        MaxHeight="45" 
                        Command="{Binding MoveDown}">
                        
                		<Button.RenderTransform>
                			<CompositeTransform Rotation="180.17"/>
                		</Button.RenderTransform>
                        
                	</Button>
					
                </StackPanel>
            </Grid>
</UserControl>