<UserControl x:Class="Smeedee.Widget.Browser.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:Views="clr-namespace:Smeedee.Widget.Browser.Views" xmlns:ViewModel="clr-namespace:Smeedee.Widget.Browser.ViewModel" mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Grid x:Name="LayoutRoot" Background="White">
        <Views:BrowserView>
            <Views:BrowserView.DataContext>
                <ViewModel:Browser/>
            </Views:BrowserView.DataContext>
        </Views:BrowserView>
    </Grid>
</UserControl>
